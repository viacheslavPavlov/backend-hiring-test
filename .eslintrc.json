{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "jest": true,
        "mocha": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "new-cap": "off",
        "consistent-return": "warn"
    }
}
